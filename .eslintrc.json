{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true, "semi": false, "endOfLine": "auto" }],
        "space-before-function-paren": ["error", "never"],
        "react/react-in-jsx-scope": "off",
        "comma-dangle": ["error", "never"]
    }
}
